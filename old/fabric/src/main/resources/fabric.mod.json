{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/syconns-super-hero-mod",
        "sources": "https://github.com/Syconn/HeroCore"
    },
    "license": "${license}",
    "icon": "hero.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "mod.syconn.hero.HeroFabric"
        ],
        "client": [
            "mod.syconn.hero.FabricClient"
        ]
    },
    "mixins": [
        "hero.mixins.json",
        "hero.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  